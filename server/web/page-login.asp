<div class="login-box ui-wrapshadow">
	<ul class="pannel">
    	<li class="active">博主登入</li>
        <li class="none">用户登入</li>
    </ul>
    <div class="login-user">
    	<div class="wrap fn-clear">
            <div class="contents">
                <div class="zone">
                    <form action="server/login.asp" method="post">
                        <div class="imgs fn-left"></div>
                        <div class="action fn-left">
                            <div class="name"></div>
                            <div class="put">
                                <table width="100%" cellpadding="0" cellspacing="0">
                                    <tr>
                                        <td>昵称：</td>
                                        <td><input type="text" value="" placeholder="请输入用户名.." name="username" /></td>
                                    </tr>
                                    <tr>
                                        <td>密码：</td>
                                        <td><input type="password" value="" placeholder="请输入密码.." name="password" /></td>
                                    </tr>
                                    <tr>
                                        <td></td>
                                        <td><input type="submit" value="验证" class="button" /> <span class="login-info">正在验证密码..请稍后!</span></td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <div class="contents"><div class="zone">2</div></div>
        </div>
    </div>
</div>